{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "Reconvierteme",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "Json-Placeholder",
      "urls": ["https://jsonplaceholder.typicode.com/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxAge": "1d",
        "maxSize": 100,
        "timeout": "3s"
      }
    }
  ]
}
